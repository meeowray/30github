namespace c__basic
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("hello world");

            int age = 25;
            if (age == 25)
            {
                Console.WriteLine("age is ok.");
                Console.ReadLine();
            }
            else
            {
                Console.WriteLine("wrong");
                Console.ReadLine();
            }
        }
    }
}
